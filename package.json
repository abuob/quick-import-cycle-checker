{
    "name": "quick-import-cycle-checker",
    "version": "0.0.1",
    "description": "Quick import cycle checker for ES6 imports",
    "main": "dist/index.js",
    "engines": {
        "node": ">= 14"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/abuob/quick-import-cycle-checker.git"
    },
    "keywords": [
        "import-cycles"
    ],
    "bin": {
        "quick-import-cycle-checker": "dist/index.js"
    },
    "files": [
        "dist/**/*",
        "schema.json",
        "graph-display-template.html"
    ],
    "author": "abuob",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/abuob/quick-import-cycle-checker/issues"
    },
    "homepage": "https://github.com/abuob/quick-import-cycle-checker#readme",
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "npm run format:write",
        "format:check": "prettier --check --loglevel warn \"**/*.+(ts|json|md|js|yml)\"",
        "format:write": "prettier --write --loglevel warn \"**/*.+(ts|json|md|js|yml)\""
    },
    "devDependencies": {
        "@types/jest": "^28.1.4",
        "@types/node": "^18.0.3",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^28.1.2",
        "prettier": "^2.7.1",
        "ts-jest": "^28.0.5",
        "typescript": "^4.7.4"
    }
}
